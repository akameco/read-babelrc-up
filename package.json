{
  "name": "read-babelrc-up",
  "version": "1.1.0",
  "description": "Read the closet Babel configuration file",
  "license": "MIT",
  "repository": "akameco/read-babelrc-up",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "https://akameco.github.io"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '*.{js,json,md}'",
    "lint": "eslint .",
    "test": "npm run lint && ava"
  },
  "files": [
    "index.js"
  ],
  "lint-staged": {
    "*.js": [
      "fmt",
      "test"
    ],
    "*.{json,md}": [
      "fmt"
    ]
  },
  "keywords": [
    "babel",
    "babelrc",
    ".babelrc",
    "babel.config.js",
    "json",
    "file",
    "parse",
    "load",
    "look-up",
    "walk",
    "path"
  ],
  "dependencies": {
    "@types/babel__core": "^7.1.12",
    "find-up": "^4.1.0",
    "json5": "^2.1.2"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "all-contributors-cli": "^6.14.0",
    "ava": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-precure": "^5.3.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "ava": {
    "babel": true
  }
}
